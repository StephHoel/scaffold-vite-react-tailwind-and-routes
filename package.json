{
  "name": "scaffold",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://stephhoel.github.io/scaffold/",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist",
    "predeploy": "npm run build",
    "format": "biome format --write",
    "lint": "biome lint --error-on-warnings",
    "ci:lint": "biome check",
    "precommit": "biome check --staged"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3",
    "vite": "^7.0.2"
  }
}
